package Face;

import org.jointheleague.graphical.robot.Robot;

public class Face
{
	public static void main(String[] args)
	{
		new Face();
	}

	public Face()
	{
		Robot tommy = new Robot();
		tommy.setSpeed(10);
		eyes(tommy);
	}

	void eyes(Robot r)
	{
		r.penDown();
		for (int i = 0; i < 360; i++)
		{
			r.move(1);
			r.turn(1);
		}
		for (int i = 0; i < 360; i++)
		{
			r.move(1);
			r.turn(-1);
		}
	}

}
