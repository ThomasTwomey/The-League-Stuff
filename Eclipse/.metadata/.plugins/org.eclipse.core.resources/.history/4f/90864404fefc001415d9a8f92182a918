
public class MasterForLoops
{
	public static void main(String[] args)
	{
		for (int i = 0; i < 6; i++)
		{
			System.out.println("*");
			
			for (int j = 0; j <= 4; j++)
			{
				if(i==0)
				{}
				
				if(i==1)
				{
					System.out.print("*");
				}
				if(i==2)
				{
					System.out.print("*");
					System.out.print("*");
				}if(i==3)
				{
					System.out.print("*");
					System.out.print("*");
					System.out.print("*");
				}if(i==4)
				{
					System.out.print("*");
					System.out.print("*");
					System.out.print("*");
					System.out.print("*");
				}if(i==5)
				{
					System.out.print("*");
					System.out.print("*");
					System.out.print("*");
					System.out.print("*");
					System.out.print("*");
				}
			}
		}
	}
}
