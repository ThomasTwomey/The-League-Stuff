}

class Student {
	int intelligence;
	String name;

	Student(int intelligence, String name) {
		this.intelligence = intelligence;
		this.name = name;
	}

	public int getIntelligence() {
		return intelligence;
	}
	public String getName() {
		return name;
	}
}

